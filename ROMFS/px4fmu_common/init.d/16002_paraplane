
#!nsh
#
# @name Paraplane
#
# @type Helicopter
# @class Copter
#
#
# @output MAIN1 left servo
# @output MAIN2 right  servo
# @output MAIN3 motor
#

sh /etc/init.d/rc.fw_defaults


# set as a plane
set MAV_TYPE 1


set MIXER paraplane


set PWM_OUT 1234
set PWM_DISARMED


if [ $AUTOCNF = yes ]
then
    param set FW_ARSP_MODE 1.0
fi